extends layout

block content
  .modal(id="display")
  .w3-card-4 
    header.w3-indigo
      h1 File Upload 
    form.w3-container(action="/files" method="POST" enctype="multipart/form-data")
      label.w3-text-teal
        b Select file 
      input.w3-input.w3-border.w3-light-grey(type="file" name="myFile")
      input.w3-btn.w3-blue-grey(type="submit" value="Send")

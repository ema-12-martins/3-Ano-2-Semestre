extends layout 

block content 
    .w3-card-4
        header.w3-container.w3-teal
            h1 Pagina do contrato #{objeto._id}

        
        .w3-container
            ul.w3-ul.w3-card-4(style="width:50%")
                li
                    b Id: 
                    | #{objeto._id}
                li
                    b Data de inicio de contrato:
                    | #{objeto.DataInicioContrato}
                li
                    b Data de final de contrato:
                    | #{objeto.DataFimContrato}
                

        footer.w3-container.w3-teal
            address
                span Gerado por Ema Martins em #{d} - [
                a(href="/") Voltar
                span ]